<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="fr.ybo.transportsrennes">
    <application android:icon="@drawable/icon" android:label="@string/app_name"
                 android:debuggable="true" android:name="TransportsRennesApplication"
                 android:theme="@android:style/Theme.NoTitleBar">
        <activity android:name=".TransportsRennes">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".BusRennes"/>
        <activity android:name=".ListAlerts"/>
        <activity android:name=".DetailAlert"/>
        <activity android:name=".ListArret"/>
        <activity android:name=".DetailArret"/>
        <activity android:name=".ListFavoris"/>
        <activity android:name=".ListStationsByPosition"/>
        <activity android:name=".ListStationsFavoris"/>
        <activity android:name=".ListArretByPosition"/>
    </application>
    <uses-sdk android:minSdkVersion="3"
              android:targetSdkVersion="8"
              android:maxSdkVersion="9"/>
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
</manifest>
